{
  "name": "bikesewa",
  "version": "1.0.0",
  "description": "Motorcycle rental website project",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass src/sass/main.scss src/css/style.css -w",
    "devserver": "live-server",
    "start": "run-p watch:sass devserver",
    "prefix": "postcss src/css/style.css --use autoprefixer -d src/css",
    "compress": "sass src/sass/main.scss:src/css/style.css --style=compressed"
  },
  "author": "Madan Budhathoki",
  "license": "ISC",
  "browserslist": "last 10 versions",
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.32",
    "postcss-cli": "^10.1.0",
    "sass": "^1.63.6"
  }
}
